{
  "name": "saasbuild",
  "version": "1.0.0",
  "description": "SaaS Builder",
  "main": "index.js",
  "scripts": {
    "test": "cypress run",
    "lint": "run-script-os",
    "lint:darwin:linux": "eslint . --ext .js,.jsx && . venv/bin/activate && flake8 && ( flake8 --select W,F || true )",
    "lint:win32": "eslint . --ext .js,.jsx && call venv\\Scripts\\activate && flake8 && ( flake8 --select W,F || exit 0 )",
    "dbreset": "run-script-os",
    "dbreset:darwin:linux": ". venv/bin/activate && flask dbcreate && flask dbseed",
    "dbreset:win32": "call venv\\Scripts\\activate && flask dbcreate && flask dbseed",
    "start": "run-script-os",
    "start:darwin:linux": ". venv/bin/activate && flask run",
    "start:win32": "call venv\\Scripts\\activate && flask run",
    "dev": "webpack --config webpack.dev.js",
    "prod": "webpack --config webpack.prod.js",
    "heroku-prebuild": "npm install",
    "heroku-postbuild": "webpack --config webpack.$env.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-proposal-decorators": "^7.7.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-brands-svg-icons": "^5.11.2",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/react-fontawesome": "^0.1.7",
    "axios": "^0.19.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "bootstrap": "^4.3.1",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "cypress": "^4.9.0",
    "cypress-dotenv": "^1.2.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.3.1",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-react": "^7.20.3",
    "history": "^4.10.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^4.0.0-beta.11",
    "jquery": "^3.5.1",
    "mini-css-extract-plugin": "^0.8.0",
    "mobx": "^5.15.0",
    "mobx-react": "^6.1.4",
    "node-sass": "^4.14.1",
    "popper.js": "^1.16.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-helmet": "^5.2.1",
    "react-router-dom": "^5.1.2",
    "run-script-os": "^1.1.1",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.1",
    "jsonpath": "^1.0.2",
    "rc-color-picker": "^1.2.6",
    "react-bootstrap": "^1.0.0-beta.16",
    "react-redux": "^7.1.3",
    "redux": "^4.0.5",
    "shortid": "^2.2.15"
  }
}
